<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" >
    <item android:state_pressed="true" >
        <shape android:shape="rectangle"  >
            <stroke android:width="1dip" android:color="#FFD3E1E3" />
            <gradient android:angle="-90" android:startColor="#FFD3E1E3" android:endColor="#FFD3E1E3"  />
        </shape>
    </item>
    <item android:state_focused="true">
        <shape android:shape="rectangle"  >
            <stroke android:width="1dip" android:color="#d3e1e3" />
            <solid android:color="#FFD3E1E3"/>
        </shape>
    </item>
</selector>